{
	"name": "yangyifan/laravel-admin",
	"description": "",
	"keywords": ["framework", "laravel", "laravel-admin", "yangyifan"],
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "yangyifan",
			"email": "yangyifanphp@gmail.com"
		}
	],
	"require": {
		"laravel/framework": "5.0.*",
        "jenssegers/agent" : "*",
        "maatwebsite/excel": "~2.0.0",
        "barryvdh/laravel-ide-helper" : "*",
        "dingo/api": "*",
        "mews/captcha": "~2.0",
        "predis/predis": "~1.1@dev",
	  	"yangyifan/upload":"dev-master",
	  	"yangyifan/laravel-editor":"*",
        "jpush/jpush": "v3.2.0",
		"gossi/php-code-generator": "dev-master"

	},
   "minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
	  	"files" : [
		  "app/Functions/common.func.php",
		  "app/Functions/base.func.php"
		],
		"psr-4": {
			"App\\": "app/",
            "Yangyifan\\AutoBuild\\" : "packages/yangyifan/auto-build/src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}

}
