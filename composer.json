{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"illuminate/html": "~5.0",
		"yuanchao/laravel-5-markdown-editor": "dev-master",
        "toplan/filter-manager": "dev-master",
        "jenssegers/agent" : "*",
        "maatwebsite/excel": "~2.0.0",
        "evaengine/eva-oauth": "~1.0",
        "overtrue/laravel-wechat":"2.0.*",
        "overtrue/laravel-pinyin": "dev-master",
        "barryvdh/laravel-ide-helper" : "*",
        "dingo/api": "*",
        "zgldh/qiniu-laravel-storage": "dev-master",
        "stevenyangecho/laravel-u-editor": "~1.1",
        "ender/laravel-ueditor":"0.8.*",
        "rcrowe/turbo": "0.2.*",
        "mews/captcha": "~2.0",
	    "hprose/hprose": "dev-master"

	},
   "minimum-stability": "dev",
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}

}
